{
    "type": "origins:multiple",
    "normal_torch": {
        "type": "origins:action_over_time",
        "interval": 100,
        "entity_action":{
            "type": "origins:if_else",
            "condition":{
                "type": "origins:and",
                "conditions":[
                    {
                        "type": "origins:resource",
                        "resource": "pheonix:heat",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    {
                        "type": "origins:resource",
                        "resource": "pheonix:cold",
                        "comparison": "!=",
                        "compare_to": 0
                    }
                ]
            },
            "if_action":{
                "type": "origins:change_resource",
                "resource": "pheonix:cold",
                "change": -3
            },
            "else_action":{
                "type": "origins:change_resource",
                "resource": "pheonix:heat",
                "change": 3
            }
        },
        "condition":{
            "type":"origins:block_in_radius",
            "radius": 6,
            "block_condition": {
                "type": "origins:or",
                "conditions":[
                    {
                        "type": "origins:block",
                        "block": "minecraft:torch"
                    },
                    {
                        "type": "origins:block",
                        "block": "minecraft:wall_torch"
                    }
                ]
            }
        }
    },


